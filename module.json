{
    "id": "samioli-module",
    "title": "Sami & Oli's Foundry Module",
    "description": "A bunch of things by Sami & Oli",
    "authors": [
      {
        "name": "Sami"
      },
      {
        "name": "Oli"
      }
    ],
    "esmodules": [
      "dist/samioli-module.js"
    ],
    "packFolders": [
        {
            "name": "SamiOli Module",
            "sorting": "a",
            "color": "#16241b",
            "packs": [
                "Macros"
            ]
        }
    ],
    "packs": [
      {
        "label": "SamiOli Macros",
        "name": "Macros",
        "path": "packs/macros.db",
        "system": "pf2e",
        "type": "Macro",
        "private": false,
        "flags": {}
      }
    ],
    "compatibility": {
      "minimum": "12",
      "verified": "12"
    },
    "relationships": {
      "requires": [
        {
          "id": "socketlib",
          "type": "module",
          "manifest": "https://github.com/farling42/foundryvtt-socketlib/releases/latest/download/module.json"
        }
      ]
    },
    "version": "#{VERSION}#",
    "url": "#{URL}#",
    "manifest": "#{MANIFEST}#",
    "download": "#{DOWNLOAD}#",
    "socket": true
}
